    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".CustomerActivity.CustomerBiddings.CustomerBidding">
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:translationZ="16dp"
        android:elevation="16dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/customercolour"
        style="@style/Base.Theme.AppCompat.Light.DarkActionBar"
        android:minHeight="?attr/actionBarSize"
        app:navigationIcon="@drawable/ic_arrow_back_whitw_24dp">
    </android.support.v7.widget.Toolbar>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

      <!-- <android.support.v7.widget.RecyclerView
           android:id="@+id/recycleview"
           android:layout_width="match_parent"
           android:layout_height="wrap_content">
       </android.support.v7.widget.RecyclerView>
-->
    <android.support.v7.widget.CardView
        android:id="@+id/cardview1"
        android:layout_marginTop="60dp"
        android:translationZ="4dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:padding="5dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/dishtype">
                    <ImageView
                        android:scaleType="centerCrop"
                        android:id="@+id/profile_image1"
                        android:background="@drawable/turkies"
                        android:layout_width="100dp"
                        android:layout_height="100dp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginLeft="15dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/wname1"
                        android:minLines="1"
                        android:maxLines="1"
                        android:textSize="20dp"
                        android:text="Turkish Food"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text="Cumin, Fenugreek, Haspir, Mahleb"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />

                </LinearLayout>



            </LinearLayout>
        </LinearLayout>

    </android.support.v7.widget.CardView>
